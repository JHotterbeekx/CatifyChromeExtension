{
  "name": "Catify",
  "description": "Worship Garfield and summon cats to take over the images in your life.",
  "manifest_version": 2,
  "version": "1.0.0",
  "author": "John Hotterbeekx",
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "browser_action": {
    "default_icon": "icons/32.png",
    "default_title": "Feeling catty today?",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
